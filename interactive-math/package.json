{
  "name": "interactive-math",
  "version": "0.1.0",
  "description": "A library for creating interactive mathematical formulas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && node fix-imports.js",
    "test": "jest",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "serve": "http-server . -p 3001",
    "dev": "vite",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "mathematics",
    "interactive",
    "formula",
    "math",
    "latex"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.4",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "mathjs": "^11.8.0",
    "mathjax-full": "^3.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
} 